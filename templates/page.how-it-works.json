        "template--16679329693941__9bab2570-0bb4-4857-86f6-8d0be8eb42e2-167466404830fd5ee2-1": {
          "type": "feature_card",
          "settings": {
            "icon_select": "icon_1",
            "card_title": "<p><strong>Play<\/strong><\/p>",
            "card_text": "The fun part!  Unbox your first kit and experience the magic of quality time spent with your child in your spruced up play space. Rest assured knowing they are getting the safest and highest quality toys on the market."
