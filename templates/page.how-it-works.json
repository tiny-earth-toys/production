        },
        "template--16679329693941__9bab2570-0bb4-4857-86f6-8d0be8eb42e2-167466404830fd5ee2-2": {
          "type": "feature_card",
          "settings": {
            "icon_select": "icon_3",
            "card_title": "<p><strong>Rotate\/Exchange<\/strong><\/p>",
            "card_text": "Every two months you'll swap out your current toys for a fresh set. Can't bear to part with one of your toys?  You always have the option to purchase any time at a discounted rate."
          }
        }
