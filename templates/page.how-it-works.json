    "0232d1a1-c205-4d71-8bc0-405c011675ba": {
      "type": "te-features-basic",
      "blocks": {
        "template--16679329693941__0232d1a1-c205-4d71-8bc0-405c011675ba-167466404830fd5ee2-0": {
          "type": "feature_card",
          "settings": {
            "icon_select": "icon_1",
            "icon_hide": false,
            "card_title": "<p>You've got mail<\/p>",
            "card_text": "Every two months, you will receive an email letting you know your next kit is being prepared. You have three days to modify your order if needed. Swap out toys, or snooze your order."
          }
        },
        "template--16679329693941__0232d1a1-c205-4d71-8bc0-405c011675ba-167466404830fd5ee2-1": {
          "type": "feature_card",
          "settings": {
            "icon_select": "icon_1",
            "icon_hide": false,
            "card_title": "<p>Ding dong<\/p>",
            "card_text": "Your new toys arrive before you send the previous kit back. These new toys were selected with your child's age and growing abilities in mind. We hope you and your little one love them!"
          }
        },
        "template--16679329693941__0232d1a1-c205-4d71-8bc0-405c011675ba-167466404830fd5ee2-2": {
          "type": "feature_card",
          "settings": {
            "icon_select": "icon_1",
            "icon_hide": false,
            "card_title": "<p>Repack & Return<\/p>",
            "card_text": "Once your new toys arrive, use the included return shipping materials (prepaid label, tape) and the original packaging to pack your kit. Within 7 days of receiving your new toys, drop your return set off at your local FedEx location."
          }
        }
      },
