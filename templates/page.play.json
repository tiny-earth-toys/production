    "beb6aa28-dd50-4950-a521-305b23113ca0": {
      "type": "te-double-column",
      "blocks": {
        "4fae37ba-7c07-408a-b0c3-1001bc8d085a": {
          "type": "column__bullet",
          "settings": {
            "bullet__hide-icon-1": true,
            "bullet__icon-1": "option_a",
            "bullet__title-1": "Child powered",
            "bullet__message-1": "All our toys are “low stimulation” toys. They never have batteries or electric power of any kind. These toys support a child’s active concentration - an essential skill to build early in life.",
            "bullet__hide-icon-2": true,
            "bullet__icon-2": "option_a",
            "bullet__title-2": "Skill Oriented",
            "bullet__message-2": "Many of our toys are skill oriented. This helps children discover and develop specific skills while having fun.",
            "bullet__hide-icon-3": true,
            "bullet__icon-3": "option_a",
            "bullet__title-3": "",
            "bullet__message-3": ""
