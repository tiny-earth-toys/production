        "template--16720487776501__52a83d38-c7ca-49aa-9c58-215efb66ab22-1679497731feb79e00-1": {
          "type": "title",
          "settings": {
            "banner__title": "<p>Education-<\/p>"
          }
        },
        "template--16720487776501__52a83d38-c7ca-49aa-9c58-215efb66ab22-1679497731feb79e00-2": {
          "type": "subtitle",
          "settings": {
            "banner__subtitle": "We believe at it's best, early childhood education, supports children as they learn about themselves and the world around them.  It gives them a strong foundation to reach their highest potential while helping to cultivate values and skills we believe are vital for our world to survive and thrive."
          }
