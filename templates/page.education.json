        "template--16720487776501__52a83d38-c7ca-49aa-9c58-215efb66ab22-1679497731feb79e00-1": {
          "type": "title",
          "settings": {
            "banner__title": "<p>Learning and development<\/p>"
          }
        },
        "template--16720487776501__52a83d38-c7ca-49aa-9c58-215efb66ab22-1679497731feb79e00-2": {
          "type": "subtitle",
          "settings": {
            "banner__subtitle": "At its best, early childhood education, supports children as they learn about themselves and the world around them.  Tiny Earth Toys believes in a child-centered, holistic approach to education that values, simplicity, independence, and self-discovery."
          }
